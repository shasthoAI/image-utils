{
  "name": "image-utils",
  "version": "1.0.0",
  "main": "src/compressor.js",
  "type": "module",
  "bin": {
    "image-compress": "./bin/compress.js",
    "image-split": "./bin/split.js",
    "pdf-split": "./bin/pdf-split.js"
  },
  "scripts": {
    "compress": "node src/compressor.js",
    "compress:medium": "node src/compressor.js --medium",
    "compress:high": "node src/compressor.js --high",
    "compress:extreme": "node src/compressor.js --extreme",
    "compress:png": "node src/compressor.js --png-optimized",
    "compress:webp": "node src/compressor.js --webp",
    "compress:png-extreme": "node src/compressor.js --png-optimized --extreme",
    "split": "node src/splitter.js",
    "split:pdf": "node src/pdf_splitter.js",
    "global:install": "npm install -g .",
    "global:uninstall": "npm uninstall -g image-utils",
    "global:update": "npm uninstall -g image-utils && npm install -g ."
  },
  "keywords": [],
  "author": "Shastho Limited <https://github.com/shasthoAI>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shasthoAI/image-utils.git"
  },
  "license": "MIT",
  "description": "A collection of lightweight Node.js utilities for common image processing tasks, including compression and splitting.",
  "dependencies": {
    "sharp": "^0.34.2",
    "dotenv": "^16.4.5",
    "node-poppler": "^8.0.1"
  }
}
